<ul class="series-tools-menu{% if home is defined %} up{% endif %}">
    <li class="bookmark" data-id="{{ seriesId }}" data-name="{{ seriesName|e('html_attr') }}">{{ ux_icon('mdi:bookmark') }}<span>{{ 'Add to a list'|trans }}</span></li>
    {#        <li class="favorite">{{ ux_icon('mdi:favorite') }}<span>{{ 'Mark as favorite'|trans }}</span></li> #}
    <li class="share" data-name="{{ seriesName|e('html_attr') }}">{{ ux_icon('mdi:share-outline') }}<span>{{ 'Share'|trans }}</span></li>
</ul>